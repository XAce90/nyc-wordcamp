{
  "name": "nyc-wordcamp",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": { 
    "build-task:scss-compile": "node-sass --source-map true 2018/sass/ -o 2018",
    "build-task:autoprefixer": "postcss 2018/*.css --use autoprefixer -d 2018",
    "sass:build": "npm-run-all -p build-task:*",
    "sass:watch": "chokidar '2018/sass/**/*.scss' -c 'npm run sass:build'",
    "dev": "npm-run-all -p sass:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/XAce90/nyc-wordcamp.git"
  },
  "keywords": [],
