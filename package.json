  },
  "homepage": "https://github.com/XAce90/nyc-wordcamp#readme",
  "dependencies": {
    "autoprefixer": "^9.0.1",
    "chokidar-cli": "^1.2.0",
    "node-sass": "^4.9.2",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^6.0.0"
  }
}
